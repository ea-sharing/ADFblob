{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "MyADFblob2023"
		},
		"fetch_csv_file_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'fetch_csv_file'"
		},
		"pythonstorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'pythonstorage'"
		},
		"trigger1_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/46144725-e67d-4db1-a949-31ae0f25fb14/resourceGroups/project_resgrp/providers/Microsoft.Storage/storageAccounts/adfblobstorageaccount1"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "MyADFblob2023_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"fetch_csv_file_connectionString": {
						"value": "[parameters('fetch_csv_file_connectionString')]"
					},
					"pythonstorage_connectionString": {
						"value": "[parameters('pythonstorage_connectionString')]"
					},
					"trigger1_properties_typeProperties_scope": {
						"value": "[parameters('trigger1_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}